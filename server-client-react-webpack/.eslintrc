{
  "globals": {
    "document": true,
    "window": true
  },
  "rules":{
    "linebreak-style": 0,
    "no-plusplus": "off",
    "no-console": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }]
  },
  "env": {
    "browser": true
  },
  "extends": ["airbnb"],
  "plugins": [
    "jsx-a11y",
    "react",
    "import"
  ]
}
